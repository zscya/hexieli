/* components/IconTabs/IconTabs.wxss */
.footer {
    position: fixed;
    bottom: 0;
    width: 100%;
    height: 95rpx; /*footer的高度*/
    background: #fff;
    z-index: 500;
    border-top: 1rpx solid #e9e9e9;
    display: flex;
    flex-direction: row;
  }
  
  .footer_list {
    width: 17%;
    height: 100%;
    text-align: center;
    padding-top: 8rpx;
    margin-left: 60rpx;
    margin-right: 62rpx;
  }
  
  .footer-image {
    width: 45%;
    height: 55%;
  }
  
  .footer-text {
    font-size: 30rpx;
    margin-top: -10rpx;
  }
  
  